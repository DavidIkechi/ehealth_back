{
    "version": 2,
    "builds": [
      {
        "src": "ehealth_hub/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      },
      {
        "src": "ehealth_hub/static/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "ehealth_hub/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "ehealth_hub/wsgi.py"
      }
    ]
  }
  